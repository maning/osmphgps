	amenity=fuel [0x2f01 resolution 18]

# 2f16	Nuvi: Truck Stop (see AUTO SERVICES below)

# 2e06  Nuvi: Shopping > Convenience (see SHOPPING) ---- isolate all 24/7 convenience store, all else should not be here (too many)




####03  TRANSIT (Nuvi:2f02 2f04 2f08 2f17) For-Hire & Public / Mass Transport 
# -----------------------------------------------------------------------------------------------------------------

# 2f02/11002	Nuvi: Auto Rental (since relatively unused in PH; may be combined with for-hire services such as tricycles)
	amenity=car_rental [0x2f02 resolution 23 default_name 'rent-a-car'] 
	amenity=tricycle_station {delete addr:street} [0x11002 resolution 23 default_name 'Tricycle'] 		
	

# 2f04/11004	Nuvi: Air Transportation 			# (should include distinct air-terminal icon, (how about helipad, airbase for emergency landings?) 

# 	PH International Airports:
	aeroway=aerodrome & 
		( name="Laoag Airport" | name="Laoag International Airport"			  	#	
		| name="Diosdado Macapagal International Airport" | name="Clark International Airport"	# Clark International Airport / DMIA
		| name="Ninoy Aquino International Airport"						# NAIA
		| name="Mactan-Cebu International Airport"						# CEB
		| name="Francisco Bangoy International Airport"						# Davao International Airport
		| name="General Santos International Airport"
		| name="Zamboanga International Airport"
		) [0x2f04 resolution 16]
	aeroway=aerodrome [0x2f04 resolution 18]
	aeroway=airport [0x2f04 resolution 18]
	aeroway=terminal [0x2f04 resolution 20]

# 2f08/11008	Nuvi: Ground Transportation (eg. Bus Station, Bus Stop, PUJ, FX, Taxi)
	amenity=bus_station [0x2f08 resolution 18]
	highway=bus_stop [0x2f08 resolution 23 default_name 'PUV Stop']
	amenity=fx_station [0x2f08 resolution 21 default_name 'FX station']
	amenity=jeepney_station [0x2f08 resolution 21 default_name 'PUJ station']
	amenity=taxi [0x2f08 resolution 21 default_name 'Taxi']

# 2f17/11017 	Nuvi: Transit Service (Mass_Transit, Ligh Rail, PNR Station, Ferry Terminals) - Assign as insivisible icon
	
#	LRT 1
	   railway=station & 
	  ((name="Baclaran" | name="EDSA" | name="Libertad" | name="Gil Puyat" |  name="Vito Cruz" | 
	    name="Quirino" | name="Pedro Gil" | name="U.N. Avenue") & addr:street="Taft Avenue" ) | 
	    name="Central Terminal" |
	  ((name="Carriedo" | name="Doroteo Jose" | name="Bambang" | name="Tayuman" | name="Blumentritt" | 
	    name="Abad Santos" | name="R. Papa" | name="5th Avenue" | name="Monumento") & addr:street="Rizal Ave" ) |
	  ((name="Balintawak" | name="Roosevelt") & addr:street="EDSA" ) 
	   {name 'LRT1 ${name}'; add railway:line=LRT1} 
#	LRT 2
	   railway=station & 
	   (name="Santolan" & addr:street="Marcos Highway") |
	  ((name="Katipunan" | name="Anonas" | name="Cubao" | name="Betty-Go Belmonte" | 
	    name="Gilmore" | name="J. Ruiz") & addr:street="Aurora Boulevard") | 
	  ((name="V. Mapa" | name="Pureza") & addr:street="Ramon Magsaysay") |
	    name="Legarda" | 
	   (name="Recto" & addr:street="Claro M. Recto Avenue") 		
	   {name 'LRT2 ${name}'; add railway:line=LRT2} 
#	LRT 3
	   railway=station & 
	   (name="North Avenue" | name="Quezon Avenue" | name="Kamuning" | name="Araneta Center-Cubao" |  
	    name="Santolan" | name="Ortigas" | name="Shaw Boulevard" | name="Boni" | name="Guadalupe" |
	    name="Buendia" | name="Ayala" | name="Magallanes" | name="Taft Avenue" ) & addr:street="EDSA" 		
	   {name 'MRT3 ${name}'; add railway:line=MRT3}	
	railway=station & railway:line=LRT1 | railway:line=LRT2 | railway:line=MRT3 [0x2f17 resolution 18 continue] 		# generic invisible Transit Icon 			
	railway=station & railway:line=LRT1 | railway:line=LRT2 | railway:line=MRT3 {delete name} [0x300a resolution 16]	   	        # Icon for LRT
	
#	note: always put "addr:street=" at the end of the line, never at the front -- tends to crash mkgmap r1867 on slow machine

#	PNR Train Stations (all other railways not on above LRT-stations are temporarily assumed as PNR stations)	
	railway=station {name 'PNR ${name}'} [0x2f17 resolution 17 continue]                              	
	railway=station {delete name} [0x300b resolution 16]				 # Adds Icon for PNR Station, but delete name to make it unsearchable			     
	railway=halt | railway=tram_stop [0x300b resolution 21]				 # is there an applicable tram_stop in PH?

# 	SEA PORTS (the hack - doesn't seem to work on gpsr. On 76csx, the pier is only visible at 120m.
	amenity=ferry_terminal [0x2f17 resolution 18 continue] 		# assigned to 'Transit Service' instead of 'Marina'
#	   Major Ports (International)
		amenity=ferry_terminal & 					# this line and the following identify Major/International Seaports
		( name="Batangas Pier" 					# International Port
		| name="Cagayan De Oro Port (Macabalan)"		# Port of Cagayan de Oro
		| name="Cebu Port" 					# Cebu Intenational Port (CIP)
		| name="Davao Port (Sasa)"				# Port of Davao
		| name="Guimaras Island"
		| name="Iligan"
		| name="Port of Iloilo"					# Iloilo Port
		| name="Jolo"
		| name="Legazpi"
		| name="Manila North Harbor"				# (NH)
		| name="Manila South Harbor"				# (SH)
		| name="Manila International Container Terminal" 	# (MICT)
		| name="Masao"
		| name="Ozamiz Port"					# Port of Ozamiz
		| name="Puerto Princesa"
		| name="San Fernando"
		| name="Subic Bay"
		| name="Zamboanga Port"					# Zamboanga International Seaport / Port of Zamboanga
		) {add waterway:port_type=sea_port; set waterway:port_type=sea_port}
		waterway:port_type=sea_port [0x4300 resolution 17]	# big sea_ports that can handle big ships
#	 	note: Major Ports based on: http://wiki.answers.com/Q/What_are_the_major_ports_in_the_Philippines & http://en.wikipedia.org/wiki/Transportation_in_the_Philippines
#	    Minor Ports (HARBOR / MARINA / RIVER_FERRY) - temporary assign all other minor ports & ferry terminals to resolution 18 
		man_made=pier & name=* {add waterway:port_type=sea_port; set waterway:port_type=sea_port} [0x2f17 resolution 21 continue]
		amenity=ferry_terminal {add waterway:port_type=sea_port; set waterway:port_type=sea_port}
		waterway:port_type=sea_port [0x4300 resolution 21]	# minor sea_ports  - for inter-island travel
		waterway:port_type=lake_port [0x4301 resolution 21]	# regular Boat Stations on Lake
		waterway:port_type=river_port [0x4302 resolution 21]	# regular Boat Stations along river (eg. private, inter-barangay)
		waterway:port_type=river_ferry [0x4303 resolution 21]	# Eg. Pasig River Ferry
#	note: seaport tag based on http://wiki.openstreetmap.org/wiki/Humanitarian_OSM_Tags/Humanitarian_Data_Model

	amenity=harbor [0x650b resolution 16]	# provides extra protection for ships against big waves during typhoon --- it's like a 'parking space' of ships, not for transport
	leisure=marina [0x2f09 resolution 18]	# for leisure/private use - not for mass/public transport (no category on nuvi) - Appears on Other>Marina/Repair&Storage on 76CSx





####04  LODGING - PLACES TO SLEEP FOR THE NIGHT (5-categories)
# -----------------------------------------------------------------------------------------------------------------
# 2b00  Nuvi: Lodging:Other (generic hotel - assigned to Business Hotel or Motel (offers <24hrs stay or short-time) <pink lodging icon>
	tourism=motel [0x2b00 resolution 21]

# 2b01  Nuvi: Lodging:Hotel/Motel <green lodging icon> (don't use for short-time business motel)
	tourism=hotel & leisure!=beach_resort [0x2b01 resolution 20]

# 2b02  Nuvi: Lodging:Bed&Breakfast_Inn <yellow lodging icon> - (short-stay,informal,dorm-type, backpacker-style accomodation) 
	tourism=guest_house [0x2b02 resolution 21]	# for PH: 'Transient House', 'Home-Stay', House or Room for Rent (for island travellers) 
	tourism=hostel [0x2b02 resolution 21]
	tourism=alpine_hut [0x2b02 resolution 21]
	tourism=chalet [0x2b02 resolution 21]

# 2b03 	Nuvi: Campground/RV_Park 0x2b03 <green tent icon> (bring-your-own-sleeping-mat)
	tourism=camp_site [0x2b03 resolution 18]
	tourism=caravan_site [0x2b03 resolution 21]

# 2b04	Nuvi: Resort <resort icon> 	(beach resort or mountain spa with overnight facilities)
	tourism=resort [0x2b04 resolution 21]
	leisure=beach_resort [0x2b04 resolution 21]	#should have lodging facilities for accomodation, otherwise, use "swimming" tag only

# Note: Not all Beaches should be considered resort (if there is no lodging facility). 
#	A none-resort 'beach' may actually have a 'Campground' facility (with simple provision for waste disposals)



####05  SHOPPING	
# -----------------------------------------------------------------------------------------------------------------
# PRE-FILTER - 24-Hour Convenience Store - regardless of tag or spelling. (mistaken for fastfood, market, variety_store, general_merchandise, etc)
 	name="7-Eleven" [0x2e06 resolution 23]
       (name="7 Eleven" | name="7Eleven" | name="7eleven" | name="711" | name="7-11" | name="7/11" | name="7 11" | name="7 - 11 Store") {set name='7-Eleven *'} [0x2e06 resolution 23]
	name="Ministop" [0x2e06 resolution 23]
	name="Mini Stop" {set name="Ministop *"} [0x2e06 resolution 23]
	name="Mightee Mart" [0x2e06 resolution 23] 
	name="Mighty Mart" {set name="Mightee Mart *"} [0x2e06 resolution 23]
	shop=convenience & operating_hours='24/7' {name'${name} (24h) **'} [0x2e03 resolution 23]
# 	-------------------------------------------------------------------

# GarminPC:Shopping:Department 0x2b01 (Anchor-Establishments inside shopping complex) <rally:RedCoatOnHanger Icon>
	shop=department_store [0x2e01 resolution 21]

# 2e02/10f02
# NOT on GarminPC menu:Supermarket 0x2e02 (Hypermart-Supermarket-MembershipShopping-WarehouseClubs-Big airconditioned groceries with no flies) <rally:Yellow Push Cart Icon>
	amenity=supermarket [0x2e02 resolution 22]
	shop=supermarket [0x2e02 resolution 22]

# 2e03/10f03
# GarminPC:Shopping:General Merchandise 0x2e03 (sari-sari, LPG dealer, daisu, japanhome, ukay-ukay, books, stationary shops) <rally:Green Bag Icon>

	shop=convenience  [0x2e03 resolution 23]	# Set 'SARI-SARI' (suking tindahan not 24hour operation) to General Merchandise
	shop=variety_store [0x2e03 resolution 23]
	shop=general | shop=general_merchandise [0x2e03 resolution 23]
	amenity=retail [0x2e03 resolution 23]
	shop=books [0x2e0a resolution 23]
	shop=stationery [0x2e0a resolution 23]
	
# 2b04 
# GarminPC:Shopping:Shopping Center 0x2b04 (malls and shopping complex covering big land area) <rally:Yellow Bag Icon>
	amenity=mall [0x2e04 resolution 21]
	shop=shopping_centre [0x2e04 resolution 21]
	shop=mall [0x2e04 resolution 21]

# 2b05
# GarminPC:Shopping:Pharmacy/Chemist 0x2b05 (Mercury Watsons and small drug stores) <rally:Rx Icon>
	amenity=pharmacy & operating_hours='24/7' { name '${name} (24h) ${branch}' | '${name} (24h)'} [0x2e05 resolution 23]
	amenity=pharmacy { name '${name} - ${branch}' | '${name}'} [0x2e05 resolution 23]
	shop=chemist [0x2e05 resolution 23]


#amenity=atm { name '${name} - ${branch}' | '${name}' | '${operator}' }


# 2e06/10f06
# GarminPC:Shopping:Convenience 0x2e06 (24hours AirConditioned Sari-Sari Stores) <rally:7-Eleven/Ministop Icon> 
#	shop=convenience [0x2e06 resolution 23]			# useless because of Pre-Filter & Gen.Merchandise filter above

